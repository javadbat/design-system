{
    "presets": [
        ["@babel/preset-env", {
            "targets": { "browsers": ["last 2 chrome versions"] },
            "useBuiltIns": "usage",
            "corejs": "3.6.5"
        }],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        "@babel/plugin-syntax-dynamic-import"
    ]
}
  
  